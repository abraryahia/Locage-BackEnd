{
  "name": "locage-backend",
  "version": "1.0.0",
  "description": "Locage BackEnd Package For an E-Commerce Project",
  "main": "app.js",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "scripts": {
    "preinstall": "npm install -g pm2",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "pm2 start app.js -i max --watch --ignore-watch='node_modules'",
    "profile": "node --prof app.js",
    "dev": "nodemon app.js",
    "fulldebug": "nodemon --dump app.js"
  },
  "keywords": [
    "MEAN",
    "Node.js",
    "Express.js",
    "MongoDB",
    "Angular"
  ],
  "author": "Team3: Mahmoud Ghonem, Mohamed Serag, Salma Rakha, Abra Yehia, Dina Waheed, Nada Tayel",
  "license": "ISC",
  "dependencies": {
