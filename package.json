    "cloudinary": "^1.25.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^9.0.2",
    "express": "^4.17.1",
    "express-validator": "^6.11.1",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.12.8",
    "mongoose-paginate-v2": "^1.3.18",
    "multer": "^1.4.2",
    "nodemailer": "^6.6.1"
  },
  "devDependencies": {
    "eslint": "^7.26.0",
